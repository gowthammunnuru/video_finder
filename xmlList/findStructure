find /studio/train/streaming -path '/studio/train/streaming/.*' -prune -or -path /studio/train/streaming/Developer_files -prune -or -path '/studio/train/streaming/*/_tmp' -prune -or -path '/studio/train/streaming/_*' -prune -or -path '/studio/train/streaming/grazia' -prune -or -path '/studio/train/streaming/business' -prune -or -path '/studio/train/streaming/*/_*' -prune -or -path '/studio/train/streaming/Python' -prune -or -path '/studio/train/streaming/python1' -prune -or -path '/studio/train/streaming/Training' -prune -or -name videoXMLData.xml -print | sed 's/videoXMLData.xml//' > fullXMLList.txt


